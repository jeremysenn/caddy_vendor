= simple_form_for(@caddy) do |f|
  = f.error_notification

  .form-inputs
    = f.input :first_name, disabled: true
    = f.input :last_name, disabled: true
    = f.input :RankingAcronym, collection: current_user.company.caddy_rankings_array, include_blank: false

  .form-actions
    = f.button :submit
